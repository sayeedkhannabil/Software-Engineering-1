<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="comp3350.grs.presentation.MainActivity">

    <fragment
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:name="comp3350.grs.presentation.LoginBackground"
            tools:layout="@layout/fragment_login_background"
            android:id="@+id/fragment"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp"></fragment>

    <SeekBar
            style="@style/Widget.AppCompat.SeekBar.Discrete"
            android:layout_width="330dp"
            android:layout_height="wrap_content"
            android:max="4"
            android:progress="1"
            android:id="@+id/seekBar"
            android:progressTint="@color/orange"
            app:layout_constraintTop_toTopOf="@+id/fragment"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="124dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.693" />

</androidx.constraintlayout.widget.ConstraintLayout>